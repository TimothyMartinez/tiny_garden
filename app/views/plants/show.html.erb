<h1>Show Products</h1>


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Light</th>
      <th>Care</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plants.each do |plant| %>
      <tr>
        <td><%= plant.name %></td>
        <td><%= plant.light %></td>
        <td><%= plant.maintenance %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Sign Up', new_user_registration_path %>
<%= link_to 'Log In', new_user_session_path %>
